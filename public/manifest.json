{
	"name": "Better Studres",
	"description": "This is an extension to improve studres appearence.",
	"version": "1.2",
	"manifest_version": 3,
	"permissions": ["storage"],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://studres.cs.st-andrews.ac.uk/*"],
			"js": ["index.bundle.js", "popup.bundle.js"]
		}
	],
	"author": "Oleksii Nazarenko",
	"action": {
		"default_popup": "popup.html"
	}
}
